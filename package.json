{
  "name": "mevn-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "export NODE_ENV=production && babel server -d prod-server && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "babel server -d prod-server --watch",
    "concurrently": "concurrently \"export NODE_ENV=development\" \"babel server -d prod-server --watch\" \"nodemon prod-server/index.js\" \"npm run serve\" "
  },
